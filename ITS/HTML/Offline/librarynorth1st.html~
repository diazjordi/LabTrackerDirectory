		<script>
			var d = new Date();
			var month = d.getMonth();
			var day = d.getDate()
			var year = d.getUTCFullYear();
			    
			var date = month + "-" + day + "-" + year;
			var time = d.toTimeString();
			    
			document.getElementById("date").innerHTML = date;
			document.getElementById("time").innerHTML = time;
		</script>


